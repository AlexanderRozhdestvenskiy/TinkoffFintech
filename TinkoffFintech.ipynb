   "source": [
    "import numpy as np\n",
    "from scipy.stats import rayleigh\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "fig, bx = plt.subplots(1, 1)\n",
    "\n",
    "tinkoff = np.linspace(rayleigh.ppf(0.0001), rayleigh.ppf(0.9999), 256)\n",
    "\n",
    "bx.plot(tinkoff, rayleigh.pdf(tinkoff), lw = 2, alpha = 1)"
   ]
  }
